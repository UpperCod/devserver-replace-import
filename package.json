{
    "name": "@uppercod/replace-import",
    "version": "0.0.0",
    "description": "utilities for the replacement of imports in js files",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "ava",
        "prepare": "npm run build && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/replace-import.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/replace-import/issues"
    },
    "homepage": "https://github.com/UpperCod/replace-import#readme",
    "dependencies": {
        "es-module-lexer": "^0.3.26"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^8.1.0",
        "ava": "^3.10.1",
        "builtin-modules": "^3.1.0",
        "esm": "^3.2.25",
        "rollup": "^2.21.0"
    },
    "typings": "types.d.ts",
    "ava": {
        "require": [
            "esm"
        ],
        "files": [
            "tests/**/*.test.js"
        ]
    },
    "exports": {
        "./*": {
            "import": "./ems/*.js",
            "require": "./cjs/*.js"
        }
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
